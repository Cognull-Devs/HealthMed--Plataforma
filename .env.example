# Supabase Configuration (Frontend - Public)
VITE_SUPABASE_URL="your_supabase_url"
VITE_SUPABASE_ANON_KEY="your_supabase_anon_key"

# Environment
VITE_APP_ENV=development

# MUX Configuration (Backend Only - Do NOT expose in frontend)
# These credentials should be set in Supabase Dashboard:
# Settings > Edge Functions > Secrets
# Add: MUX_TOKEN_ID and MUX_TOKEN_SECRET
# 
# For local development with Supabase CLI, create .env file in supabase/ folder:
MUX_TOKEN_ID="your_mux_token_id"
MUX_TOKEN_SECRET="your_mux_token_secret"
